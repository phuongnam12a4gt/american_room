<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_purple_pale</item>
        <item name="colorPrimaryVariant">@color/color_purple_dart</item>
        <item name="colorOnPrimary">@color/color_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/color_teal_pale</item>
        <item name="colorSecondaryVariant">@color/color_teal_dark</item>
        <item name="colorOnSecondary">@color/color_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="CustomStyleBottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="colorPrimary">@color/color_blue_107</item>
        <item name="colorOnSurface">@color/color_black</item>
    </style>

    <style name="RatingBar" parent="Widget.AppCompat.RatingBar">
        <item name="colorAccent">#6200ea</item>
        <item name="android:colorControlNormal">#f9a825</item>
    </style>
</resources>
